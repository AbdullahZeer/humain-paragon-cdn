{
  "spacing": {
    "$type": "dimension",
    "0": { "$value": "0" },
    "1": { "$value": "4px" },
    "2": { "$value": "8px" },
    "3": { "$value": "12px" },
    "4": { "$value": "16px" },
    "6": { "$value": "24px" },
    "8": { "$value": "32px" },
    "12": { "$value": "48px" },
    "16": { "$value": "64px" }
  },
  "radius": {
    "$type": "dimension",
    "sm": { "$value": "0.25rem" },
    "md": { "$value": "0.375rem" },
    "lg": { "$value": "0.5rem" },
    "xl": { "$value": "1rem" },
    "full": { "$value": "999rem" }
  },
  "shadow": {
    "$type": "shadow",
    "sm": { "$value": "0 1px 2px 0 rgba(0,0,0,0.05)" },
    "md": { "$value": "0 4px 6px -1px rgba(0,0,0,0.1), 0 2px 4px -2px rgba(0,0,0,0.1)" }
  }
}
